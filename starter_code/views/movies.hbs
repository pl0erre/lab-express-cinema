<main class="main-movies">

  <div class="main-title">
    <h2>Ironhack Cinema</h2>
    <p>Click on the movie to check the showtimes!</p>
  </div>
  
  <div class="movies-container">
    {{#each movies}}
    <div class="movies-cart">
      <h3>{{title}}</h3>
      <img src={{image}}>
    </div>
    {{/each}}
  </div>
  
  
    
  
</main>